{
	"name": "@base-framework/ui",
	"version": "0.0.0",
	"description": "This is a UI package that adds components and atoms that use Tailwind CSS and a theme based on Shadcn.",
	"main": "./dist/ui.js",
	"scripts": {
		"build": "node ./esbuild.js && tsc",
		"prepublishOnly": "node ./esbuild.js"
	},
	"type": "module",
	"keywords": [
		"JavaScript",
		"Framework",
		"Base",
		"ES6",
		"UI"
	],
	"author": "Chris Durfee",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chrisdurfee/ui.git"
	},
	"bugs": {
		"url": "https://github.com/chrisdurfee/ui/issues"
	},
	"devDependencies": {
		"esbuild": "^0.15.7",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"@base-framework/base": "^3.0.122",
		"@base-framework/atoms": "^1.0.20",
		"@base-framework/organisms": "1.0.54"
	},
	"types": "dist/types/ui.d.ts",
	"files": [
		"pacakge.json",
		"readme.md",
		"dist"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/ui.js",
			"require": "./dist/ui.js"
		}
	},
	"homepage": "https://github.com/chrisdurfee/organisms#readme"
